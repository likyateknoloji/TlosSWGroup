<?xml version="1.0"?>
<ws:webServiceList xmlns:com="http://www.likyateknoloji.com/XML_common_types" xmlns:ws="http://www.likyateknoloji.com/XML_web_service_types" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.likyateknoloji.com/XML_web_service_types ../xsds/tlosSWSJWebService_v_1_0.xsd">
   <ws:webServiceDefinition ID="1">
        <ws:serviceName>GlobalWeather</ws:serviceName>
        <ws:description>İstanbul'un hava durumu</ws:description>
        <ws:wsdlAddress>http://www.webservicex.net/globalweather.asmx?WSDL</ws:wsdlAddress>
        <ws:bindingList>
            <ws:binding>
                <ws:bindingName>SOAP1.1</ws:bindingName>
                <ws:operation>
                    <ws:operationName>GetWeather</ws:operationName>
                    <ws:parameterList>
                        <ws:parameter>
                            <ws:parameterName>cityName</ws:parameterName>
                            <ws:parameterType>class java.lang.String</ws:parameterType>
                            <ws:parameterValue>Turkey</ws:parameterValue>
                            <ws:isEnum>false</ws:isEnum>
                        </ws:parameter>
                        <ws:parameter>
                            <ws:parameterName>countryName</ws:parameterName>
                            <ws:parameterType>class java.lang.String</ws:parameterType>
                            <ws:parameterValue>Istanbul</ws:parameterValue>
                            <ws:isEnum>false</ws:isEnum>
                        </ws:parameter>
                    </ws:parameterList>
                </ws:operation>
            </ws:binding>
        </ws:bindingList>
        <com:userId>1</com:userId>
    </ws:webServiceDefinition>
    <ws:webServiceDefinition ID="2">
        <ws:serviceName>GlobalWeatherTest</ws:serviceName>
        <ws:description>Türkiye'nin şehirleri</ws:description>
        <ws:wsdlAddress>http://www.webservicex.net/globalweather.asmx?WSDL</ws:wsdlAddress>
        <ws:bindingList>
            <ws:binding>
                <ws:bindingName>SOAP1.1</ws:bindingName>
                <ws:operation>
                    <ws:operationName>GetCitiesByCountry</ws:operationName>
                    <ws:parameterList>
                        <ws:parameter>
                            <ws:parameterName>countryName</ws:parameterName>
                            <ws:parameterType>class java.lang.String</ws:parameterType>
                            <ws:parameterValue>Turkey</ws:parameterValue>
                            <ws:isEnum>false</ws:isEnum>
                        </ws:parameter>
                    </ws:parameterList>
                </ws:operation>
            </ws:binding>
        </ws:bindingList>
        <com:userId>1</com:userId>
    </ws:webServiceDefinition>
</ws:webServiceList>